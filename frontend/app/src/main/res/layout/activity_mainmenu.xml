<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <FrameLayout
        android:id="@+id/frameLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#c3defa">

        <FrameLayout
            android:layout_width="201dp"
            android:layout_height="243dp"
            android:background="#FDFCFC"
            android:translationX="262dp"
            android:translationY="375dp">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="139dp">

                <ImageView
                    android:id="@+id/imageView5"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    app:srcCompat="@drawable/quest2" />
            </FrameLayout>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="118dp"
                android:background="#FFFFFF"
                android:translationY="135dp">

                <TextView
                    android:id="@+id/textView9"
                    android:layout_width="173dp"
                    android:layout_height="93dp"
                    android:text="Кемеровский областной театр драмы имени А. В. Луначарского был основан 1 ноября 1934 года. Изначально театр размещался в здании"
                    android:textColor="#050505"
                    android:textSize="12sp"
                    android:translationX="15dp"
                    android:translationY="15dp" />
            </FrameLayout>

        </FrameLayout>

        <FrameLayout
            android:layout_width="368dp"
            android:layout_height="253dp"
            android:background="#FDFCFC"
            android:translationX="22dp"
            android:translationY="65dp">

            <ImageView
                android:id="@+id/imageView6"
                android:scaleType="fitXY"
                android:layout_width="match_parent"
                android:layout_height="167dp"
                app:srcCompat="@drawable/quest1" />

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="98dp"
                android:background="#FFFFFF"
                android:translationY="155dp">

                <TextView
                    android:id="@+id/textView9"
                    android:layout_width="332dp"
                    android:layout_height="49dp"
                    android:text="Задание недели - "
                    android:textColor="#050505"
                    android:translationX="15dp"
                    android:translationY="15dp" />
            </FrameLayout>

        </FrameLayout>

        <FrameLayout
            android:layout_width="201dp"
            android:layout_height="243dp"
            android:background="#FDFCFC"
            android:translationX="22dp"
            android:translationY="375dp">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="139dp">

                <ImageView
                    android:id="@+id/imageView5"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    app:srcCompat="@drawable/quest2" />
            </FrameLayout>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="118dp"
                android:background="#FFFFFF"
                android:translationY="135dp">

                <TextView
                    android:id="@+id/textView9"
                    android:layout_width="173dp"
                    android:layout_height="93dp"
                    android:text="Музыкальный театр Кузбасса имени А. К. Боброва Музыкальный театр Кузбасса имени А. К. Боброва — это театр, который предлагает..."
                    android:textColor="#050505"
                    android:textSize="12sp"
                    android:translationX="15dp"
                    android:translationY="15dp" />
            </FrameLayout>

        </FrameLayout>

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="57dp"
            android:layout_height="58dp"
            android:foregroundTint="#7631E9"
            android:src="@drawable/ic_action_name"
            app:srcCompat="@drawable/ic_action_name" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Гид архитектуры"
            android:textAppearance="@style/TextAppearance.AppCompat.Body2"
            android:textColor="#000000"
            android:textColorLink="#7631E9"
            android:textSize="24sp"
            android:translationX="65dp"
            android:translationY="13dp" />

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="67dp"
            android:background="#FDFCFC"
            android:translationY="664dp">

            <TextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Сделать фото"
                android:textSize="10sp"
                android:translationX="170dp"
                android:translationY="45dp" />

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="40dp"
                android:layout_height="36dp"
                android:translationX="327dp"
                android:translationY="10dp"
                app:srcCompat="@drawable/mapicon" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Карта"
                android:textSize="10sp"
                android:translationX="335dp"
                android:translationY="45dp" />

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="40dp"
                android:layout_height="36dp"
                android:translationX="57dp"
                android:translationY="10dp"
                app:srcCompat="@drawable/progressicon" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Прогресс"
                android:textSize="10sp"
                android:translationX="55dp"
                android:translationY="45dp" />
        </FrameLayout>

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="95dp"
            android:layout_height="82dp"
            android:translationX="155dp"
            android:translationY="630dp"
            app:srcCompat="@drawable/photoicon" />

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:translationY="330dp">

            <TextView
                android:id="@+id/textView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Опера и комедия"
                android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                android:textColor="#323435"
                android:textColorLink="#7631E9"
                android:textSize="24sp"
                android:translationX="22dp"
                android:translationY="3dp" />
        </FrameLayout>

    </FrameLayout>

</androidx.constraintlayout.widget.ConstraintLayout>